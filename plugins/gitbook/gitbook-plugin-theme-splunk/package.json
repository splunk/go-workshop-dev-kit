{
  "name": "gitbook-plugin-theme-splunk",
  "description": "A configurable theme for GitBook",
  "main": "./index.js",
  "version": "1.0.7",
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.18.12",
    "file-loader": "^6.0.0",
    "font-awesome": "4.6.3",
    "jquery": "3.1.1",
    "jsdom": "^14.0.0",
    "merge-stream": "^1.0.1",
    "mousetrap": "^1.6.1",
    "node-sass": "^4.14.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^9.0.3",
    "showdown": "^1.9.0",
    "survey-react": "^1.0.78",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "/bin/sh build.sh"
  },
  "license": "Apache-2.0",
  "gitbook": {
    "properties": {
      "styles": {
        "type": "object",
        "title": "Custom Stylesheets",
        "properties": {
          "website": {
            "title": "Stylesheet for website output",
            "default": "styles/website.css"
          }
        }
      },
      "login": {
        "type": "boolean",
        "title": "Login UI",
        "default": false
      },
      "showLevel": {
        "type": "boolean",
        "title": "Show level indicator in TOC",
        "default": false
      }
    }
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "ejs": "^2.6.1",
    "prismjs": "^1.21.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-simple-code-editor": "^0.11.0",
    "styled-components": "^4.3.2",
    "underscore": "^1.9.1"
  }
}
